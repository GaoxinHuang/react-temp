{
  "name": "react-temp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco  start",
    "build": "craco build",
    "test": "craco test",
    "test:check": "craco test --watchAll=false",
    "test:coverage": "craco test --silent --coverage --runInBand --watchAll=false",
    "prod": "yarn build && serve -s build",
    "lint": "eslint src --fix",
    "lint:check": "eslint src",
    "format": "prettier --list-different --write \"**/*.{ts,tsx,html,scss,css,js,json,md}\"",
    "format:check": "prettier --list-different \"**/*.{ts,tsx,html,scss,css,js,json,md}\"",
    "eject": "react-scripts eject",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-private-property-in-object": "^7.24.7",
    "@craco/craco": "^7.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^15.0.6",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "react-scripts": "5.0.1",
    "serve": "^14.2.3",
    "typescript": "^4.4.2"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,css,scss}": [
      "yarn run format",
      "yarn run lint:check"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
